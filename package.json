    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@yamada-ui/cli": "1.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cspell": "^8.14.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
