{
  "name": "yamada-theme",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.6.0+sha256.dae0f7e822c56b20979bb5965e3b73b8bdabb6b8b8ef121da6d857508599ca35",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "next dev",
    "build": "next build",
    "build:profile": "next build --profile",
    "start": "next start",
    "lint": "eslint . --max-warnings=0 --cache",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "typecheck": "tsc --noEmit",
    "spellcheck": "cspell \"**/*.{js,jsx,mjs,ts,tsx,mts,md,mdx}\" --quiet",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "quality": "pnpm format:check && pnpm lint && pnpm typecheck",
    "clean": "rimraf .next",
    "clean:all": "rimraf .next node_modules",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --coverage",
    "test:ui": "vitest --ui --watch --coverage"
  },
  "dependencies": {
